der{background:#ef4444;border-radius:3px}\n.landing-preview .hero{background:#f97316;border-radius:3px}\n.landing-preview .footer{background:#dc2626;border-radius:3px}\n\n/* Color Previews */\n.color-preview{width:100%;height:100%;display:flex}\n.color-strip{flex:1;display:flex;align-items:center;justify-content:center;color:white;font-weight:bold;font-size:.7rem;text-shadow:1px 1px 2px rgba(0,0,0,.5)}\n.blue-preview .color-strip:nth-child(1){background:#1e40af}\n.blue-preview .color-strip:nth-child(2){background:#3b82f6}\n.blue-preview .color-strip:nth-child(3){background:#60a5fa}\n.blue-preview .color-strip:nth-child(4){background:#93c5fd}\n.purple-preview .color-strip:nth-child(1){background:#7c3aed}\n.purple-preview .color-strip:nth-child(2){background:#a855f7}\n.purple-preview .color-strip:nth-child(3){background:#c084fc}\n.purple-preview .color-strip:nth-child(4){background:#ddd6fe}\n.green-preview .color-strip:nth-child(1){background:#065f46}\n.green-preview .color-strip:nth-child(2){background:#059669}\n.green-preview .color-strip:nth-child(3){background:#10b981}\n.green-preview .color-strip:nth-child(4){background:#34d399}\n.orange-preview .color-strip:nth-child(1){background:#c2410c}\n.orange-preview .color-strip:nth-child(2){background:#ea580c}\n.orange-preview .color-strip:nth-child(3){background:#fb923c}\n.orange-preview .color-strip:nth-child(4){background:#fed7aa}\n.gray-preview .color-strip:nth-child(1){background:#374151}\n.gray-preview .color-strip:nth-child(2){background:#6b7280}\n.gray-preview .color-strip:nth-child(3){background:#9ca3af}\n.gray-preview .color-strip:nth-child(4){background:#d1d5db}\n\n/* Feature Previews */\n.feature-preview{width:100%;height:100%;background:#f8fafc;display:flex;flex-direction:column;align-items:center;justify-content:center;gap:15px}\n.feature-icon{width:50px;height:50px;border-radius:50%;display:flex;align-items:center;justify-content:center;font-size:1.5rem;color:white;animation:pulse 2s infinite}\n@keyframes 