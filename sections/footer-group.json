btn{background:linear-gradient(135deg,#ffd700,#ffed4e);color:#8b4513}\n\n/* Responsive */\n@media (max-width:768px){\n.hero-title{font-size:2.5rem}\n.adventure-console{margin:10px}\n.console-header{flex-direction:column;gap:15px;text-align:center}\n.character-section{flex-direction:column;text-align:center}\n.character-speech::before{display:none}\n.quest-options{grid-template-columns:1fr}\n.realm-grid{grid-template-columns:1fr}\n.packages-showcase{grid-template-columns:1fr}\n.treasure-title{font-size:2rem}\n}\n</style>\n\n<script>\n// Game Data\nconst adventures = {\n  business_type: {\n    title: \"üè∞ Choose Your Kingdom\",\n    character: \"üëë\",\n    speech: \"Welcome, noble entrepreneur! What type of kingdom do you rule?\",\n    options: [\n      {value: 'startup', label: 'üöÄ Startup Realm', desc: 'A new kingdom full of potential', weight: {basic: 3, standard: 2, premium: 1}},\n      {value: 'small_business', label: 'üè™ Merchant Guild', desc: 'Established trade and commerce', weight: {basic: 2, standard: 3, premium: 2}},\n      {value: 'medium_business', label: 'üèõÔ∏è Noble House', desc: 'Growing influence and power', weight: {basic: 1, standard: 2, premium: 3}},\n      {value: 'enterprise', label: 'üè∞ Great Empire', desc: 'Vast dominion and resources', weight: {basic: 0, standard: 1, premium: 3}}\n    ]\n  },\n  website_purpose: {\n    title: \"‚öîÔ∏è Choose Your Quest\",\n    character: \"üó°Ô∏è\",\n    speech: \"Brave warrior! What is your primary mission in the digital realm?\",\n    options: [\n      {value: 'showcase', label: 'üé® Gallery Quest', desc: 'Showcase your legendary works', weight: {basic: 3, standard: 2, premium: 1}},\n      {value: 'ecommerce', label: 'üí∞ Merchant Quest', desc: 'Build a thriving marketplace', weight: {basic: 1, standard: 2, premium: 3}},\n      {value: 'lead_generation', label: 'üéØ Hunter Quest', desc: 'Capture valuable leads', weight: {basic: 2, standard: 3, premium: 2}},\n      {value: 'complex_platform', 