t-btn::before {\n      content: '';\n      position: absolute;\n      top: 0;\n      left: -100%;\n      width: 100%;\n      height: 100%;\n      background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);\n      transition: left 0.5s;\n    }\n    \n    .submit-btn:hover {\n      transform: translateY(-3px);\n      box-shadow: 0 15px 40px rgba(233, 30, 99, 0.4);\n    }\n    \n    .submit-btn:hover::before {\n      left: 100%;\n    }\n    \n    .success-message {\n      background: linear-gradient(135deg, #c8e6c9 0%, #a5d6a7 100%);\n      color: #2e7d32;\n      padding: 1.5rem;\n      border-radius: 20px;\n      margin-bottom: 2rem;\n      border: 2px solid #81c784;\n      text-align: center;\n      font-weight: 500;\n    }\n    \n    .error-message {\n      background: linear-gradient(135deg, #ffcdd2 0%, #ef9a9a 100%);\n      color: #c62828;\n      padding: 1.5rem;\n      border-radius: 20px;\n      margin-bottom: 2rem;\n      border: 2px solid #e57373;\n    }\n    \n    .error {\n      color: #e91e63;\n      font-size: 0.875rem;\n      margin-top: 0.5rem;\n      display: block;\n      font-weight: 500;\n    }\n    \n    .floating-hearts {\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      pointer-events: none;\n      overflow: hidden;\n    }\n    \n    .floating-hearts::before,\n    .floating-hearts::after {\n      content: '💕';\n      position: absolute;\n      font-size: 2rem;\n      animation: float 6s ease-in-out infinite;\n      opacity: 0.3;\n    }\n    \n    .floating-hearts::before {\n      top: 20%;\n      left: 10%;\n      animation-delay: 0s;\n    }\n    \n    .floating-hearts::after {\n      top: 60%;\n      right: 15%;\n      animation-delay: 3s;\n    }\n    \n    @keyframes float {\n      0%, 100% { transform: translateY(0px) rotate(0deg); }\n      50% { transform: translateY(-20px) rotate(180deg); }\n    }\n    \n    @media (max-width: 768px) {\n      .cute-form-container {\n        margin: 1rem;\n        padding: 2rem;\n      }\n      \n      .form-title {\n        font-size: 2.2rem;\n      }\n      \n      .theme-grid {\n        grid-template-columns: 1fr;\n      }\n      \n      .checkbox-group {\n        grid-template-columns: 1fr;\n      }\n    }\n  </style>\n\n  <div class=\"floating-hearts\"></div>\n\n  <div class=\"form-header\">\n    <h1 class=\"form-title\">Let's Create Your Dream Website! 🌸</h1>\n    <div class=\"cute-hearts\">💖 ✨ 🦄 ✨ 💖</div>\n    <p class=\"form-subtitle\">Tell us about your magical vision and we'll make it absolutely gorgeous! 💕</p>\n  </div>\n\n  <!-- Show success/error messages -->\n  {% if form.posted_successfully? %}\n    <div class=\"success-message\">\n      🎉 Yay! Your adorable website preferences have been submitted successfully! We'll contact you within 24 hours with lots of love! 💕✨\n    </div>\n  {% endif %}\n\n  {% if form.errors %}\n    <div class=\"error-message\">\n      💔 Oops! Please fix these little issues:\n      <ul>\n        {% for field in form.errors %}\n          <li>{{ field | first | capitalize }} {{ field | last }}</li>\n        {% endfor %}\n      </ul>\n    </div>\n  {% endif %}\n\n  {% form 'contact', class: 'liquid-form' %}\n    \n    <input type=\"hidden\" name=\"contact[subject]\" value=\"🌸 Cute Website Design Request - {{ customer.email | default: 'New Customer' }}\">\n    <input type=\"hidden\" name=\"contact[form_type]\" value=\"cute_website_design_form\">\n    <input type=\"hidden\" name=\"contact[timestamp]\" value=\"{{ 'now' | date: '%B %d, %Y at %I:%M %p' }}\">\n\n    <!-- Personal Information -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🌺</span>\n        Tell Us About You, Gorgeous!\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_name\">Your Beautiful Name *</label>\n        <input type=\"text\" \n               id=\"contact_name\" \n           