ures=null;updateProgress();askNextQuestion()}\n\nfunction resetConsultation(){confirm('Start over?')&&location.reload()}\n\nfunction updateProgress(){const progress=(currentQuestionIndex/questions.length)*100;document.getElementById('progressBar').style.width=progress+'%';const texts=['Getting started...','Understanding business...','Exploring purpose...','Choosing theme...','Selecting layout...','Picking colors...','Choosing features...','Reviewing budget...','Planning timeline...','Finalizing...'];document.getElementById('progressText').textContent=texts[Math.min(currentQuestionIndex,texts.length-1)]}\n\nfunction askNextQuestion(){if(currentQuestionIndex>=questions.length){showRecommendation();return}const question=questions[currentQuestionIndex];addBotMessage(question.question);setTimeout(()=>{if(question.type==='theme_selection')showThemeSelection();else if(question.type==='layout_selection')showLayoutSelection();else if(question.type==='color_selection')showColorSelection();else if(question.type==='features_selection')showFeaturesSelection();else showOptions(question.options)},500)}\n\nfunction addBotMessage(message){const chatMessages=document.getElementById('chatMessages');const messageDiv=document.createElement('div');messageDiv.className='message bot-message';messageDiv.innerHTML=`<div class=\"message-content\">${message}</div>`;chatMessages.appendChild(messageDiv);chatMessages.scrollTop=chatMessages.scrollHeight}\n\nfunction addUserMessage(message){const chatMessages=document.getElementById('chatMessages');const messageDiv=document.createElement('div');messageDiv.className='message user-message';messageDiv.innerHTML=`<div class=\"message-content\">${message}</div>`;chatMessages.appendChild(messageDiv);chatMessages.scrollTop=chatMessages.scrollHeight}\n\nfunction showOptions(options){const chatInput=document.getElementById('chatInput');const optionsDiv=document.createElement('div');optionsDiv.className='option-buttons';options.forEach(option=>{const button=document.createElement('button');button.className='option-button';button.textContent=option.label;button.onclick=()=>selectOption(option);optionsDiv.appendChild(button)});chatInput.innerHTML='';chatInput.appendChild(optionsDiv)}\n\nfunction createExample(type,key){const examples={theme:{'corporate-theme':`<div class=\"theme-example corporate-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`,'creative-theme':`<div class=\"theme-example creative-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`,'ecommerce-theme':`<div class=\"theme-example ecommerce-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`,'minimal-theme':`<div class=\"theme-example minimal-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`,'tech-theme':`<div class=\"theme-example tech-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`,'portfolio-theme':`<div class=\"theme-example portfolio-theme\"><div class=\"theme-header\"><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div><div class=\"theme-dot\"></div></div><div class=\"theme-content\"><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div><div class=\"theme-block\"></div></div></div>`},layout:{'header-sidebar-layout':`<div class=\"layout-example\"><div class=\"header-sidebar-layout\"><div class=\"header\"></div><div class=\"main\"><div class=\"sidebar\"></div><div class=\"content\"></div></div></div></div>`,'full-width-layout':`<div class=\"layout-example\"><div class=\"full-width-layout\"><div class=\"block\"></div></div></div>`,'grid-showcase-layout':`<div class=\"layout-example\"><div class=\"grid-showcase-layout\"><div class=\"block\"></div><div class=\"block\"></div><div class=\"block\"></div><div class=\"block\"></div><div class=\"block\"></div><div class=\"block\"></div></div></div>`,'landing-page-layout':`<div class=\"layout-example\"><div class=\"landing-page-layout\"><div class=\"header\"></div><div class=\"hero\"></div><div class=\"footer\"></div></div></div>`},color:{'blue-scheme':`<div class=\"color-example blue-scheme\"><div class=\"color-strip\">DEEP</div><div class=\"color-strip\">BLUE</div><div class=\"color-strip\">LIGHT</div><div class=\"color-strip\">SOFT</div></div>`,'purple-scheme':`<div class=\"color-example purple-scheme\"><div class=\"color-strip\">DEEP</div><div class=\"color-strip\">PURPLE</div><div class=\"color-strip\">LIGHT</div><div class=\"color-strip\">SOFT</div></div>`,'green-scheme':`<div class=\"color-example green-scheme\"><div class=\"color-strip\">DARK</div><div class=\"color-strip\">GREEN</div><div class=\"color-strip\">FRESH</div><div class=\"color-strip\">LIGHT</div></div>`,'orange-scheme':`<div class=\"color-example orange-scheme\"><div class=\"color-strip\">DEEP</div><div class=\"color-strip\">ORANGE</div><div class=\"color-strip\">BRIGHT</div><div class=\"color-strip\">LIGHT</div></div>`,'gray-scheme':`<div class=\"color-example gray-scheme\"><div class=\"color-strip\">DARK</div><div class=\"color-strip\">GRAY</div><div class=\"color-strip\">MEDIUM</div><div class=\"color-strip\">LIGHT</div></div>`},features:{'basic-features':`<div class=\"features-example basic-features\"><div class=\"feature-icon\">📄</div><div class=\"feature-text\">Essential Features</div></div>`,'ecommerce-features':`<div class=\"features-example ecommerce-features\"><div class=\"feature-icon\">🛒</div><div class=\"feature-text\">E-commerce Ready</div></div>`,'advanced-features':`<div class=\"features-example advanced-features\"><div class=\"feature-icon\">⚡</div><div class=\"feature-text\">Advanced Features</div></div>`,'custom-features':`<div class=\"features-example custom-features\"><div class=\"feature-icon\">🔧</div><div class=\"feature-text\">Custom Development</div></div>`}};return examples[type][key]||''}\n\nfunction showThemeSelection(){const grid=document.getElementById('themesGrid');grid.innerHTML='';Object.keys(websiteThemes).forEach(key=>{const theme=websiteThemes[key];const card=document.createElement('div');card.className='option-card';card.onclick=()=>selectTheme(key);card.innerHTML=`${createExample('theme',theme.example)}<div class=\"option-info\"><h4>${theme.name}</h4><p>${theme.description}</p><div class=\"option-tags\">${theme.tags.map(tag=>`<span class=\"option-tag\">${tag}</span>`).join('')}</div></div>`;grid.appendChild(card)});document.getElementById('themeModal').style.display='flex'}\n\nfunction showLayoutSelection(){const grid=document.getElementById('layoutsGrid');grid.innerHTML='';Object.keys(layoutOptions).forEach(key=>{const layout=layoutOptions[key];const card=document.createElement('div');card.className='option-card';card.onclick=()=>selectLayout(key);card.innerHTML=`${createExample('layout',layout.example)}<div class=\"option-info\"><h4>${layout.name}</h4><p>${layout.description}</p><div class=\"option-tags\">${layout.tags.map(tag=>`<span class=\"option-tag\">${tag}</span>`).join('')}</div></div>`;grid.appendChild(card)});document.getElementById('layoutModal').style.display='flex'}\n\nfunction showColorSelection(){const grid=document.getElementById('colorsGrid');grid.innerHTML='';Object.keys(colorSchemes).forEach(key=>{const color=colorSchemes[key];const card=document.createElement('div');card.className='option-card';card.onclick=()=>selectColor(key);card.innerHTML=`${createExample('color',color.example)}<div class=\"option-info\"><h4>${color.name}</h4><p>${color.description}</p><div class=\"option-tags\">${color.tags.map(tag=>`<span class=\"option-tag\">${tag}</span>`).join('')}</div></div>`;grid.appendChild(card)});document.getElementById('colorModal').style.display='flex'}\n\nfunction showFeaturesSelection(){const grid=document.getElementById('featuresGrid');grid.innerHTML='';Object.keys(featureOptions).forEach(key=>{const features=featureOptions[key];const card=document.createElement('div');card.className='option-card';card.onclick=()=>selectFeatures(key);card.innerHTML=`${createExample('features',features.example)}<div class=\"option-info\"><h4>${features.name}</h4><p>${features.description}</p><div class=\"option-tags\">${features.tags.map(tag=>`<span class=\"option-tag\">${tag}</span>`).join('')}</div></div>`;grid.appendChild(card)});document.getElementById('featuresModal').style.display='flex'}\n\nfunction selectTheme(key){selectedTheme=key;const theme=websiteThemes[key];Object.keys(theme.weight).forEach(type=>{packageScores[type]+=theme.weight[type]});document.querySelectorAll('.option-card').forEach(card=>card.classList.remove('selected'));event.currentTarget.classList.add('selected');setTimeout(()=>{closeModal('themeModal');addUserMessage(`${theme.name} - ${theme.description}`);currentQuestionIndex++;updateProgress();setTimeout(askNextQuestion,800)},1000)}\n\nfunction selectLayout(key){selectedLayout=key;const layout=layoutOptions[key];Object.keys(layout.weight).forEach(type=>{packageScores[type]+=layout.weight[type]});document.querySelectorAll('.option-card').forEach(card=>card.classList.remove('selected'));event.currentTarget.classList.add('selected');setTimeout(()=>{closeModal('layoutModal');addUserMessage(`${layout.name} - ${layout.description}`);currentQuestionIndex++;updateProgress();setTimeout(askNextQuestion,800)},1000)}\n\nfunction selectColor(key){selectedColor=key;const color=colorSchemes[key];Object.keys(color.weight).forEach(type=>{packageScores[type]+=color.weight[type]});document.querySelectorAll('.option-card').forEach(card=>card.classList.remove('selected'));event.currentTarget.classList.add('selected');setTimeout(()=>{closeModal('colorModal');addUserMessage(`${color.name} - ${color.description}`);currentQuestionIndex++;updateProgress();setTimeout(askNextQuestion,800)},1000)}\n\nfunction selectFeatures(key){selectedFeatures=key;const features=featureOptions[key];Object.keys(features.weight).forEach(type=>{packageScores[type]+=features.weight[type]});document.querySelectorAll('.option-card').forEach(card=>card.classList.remove('selected'));event.currentTarget.classList.add('selected');setTimeout(()=>{closeModal('featuresModal');addUserMessage(`${features.name} - ${features.description}`);currentQuestionIndex++;updateProgress();setTimeout(askNextQuestion,800)},1000)}\n\nfunction closeModal(id){document.getElementById(id).style.display='none'}\n\nfunction selectOption(option){const question=questions[currentQuestionIndex];userAnswers[question.id]=option.value;Object.keys(option.weight).forEach(type=>{packageScores[type]+=option.weight[type]});addUserMessage(option.label);document.getElementById('chatInput').innerHTML='';currentQuestionIndex++;updateProgress();setTimeout(askNextQuestion,800)}\n\nfunction showRecommendation(){const recommendedPackage=Object.keys(packageScores).reduce((a,b)=>packageScores[a]>packageScores[b]?a:b);addBotMessage(\"Excellent! Based on your selections, I've prepared personalized web development recommendations:\");setTimeout(()=>{displayPackages(recommendedPackage);updateProgress();document.getElementById('progressText').textContent='Recommendations ready'},1000)}\n\nfunction displayPackages(recommendedPackage){const container=document.getElementById('resultsContainer');const grid=document.getElementById('packagesGrid');if(selectedTheme||selectedLayout||selectedColor||selectedFeatures){const preview=document.getElementById('selectedPreview');let previewHTML='<h4>Your Website Selections:</h4><div style=\"display:flex;flex-wrap:wrap;gap:10px;\">';if(selectedTheme)previewHTML+=`<span style=\"background:#e0f2fe;color:#0369a1;padding:5px 10px;border-radius:15px;font-size:0.8rem;\">${websiteThemes[selectedTheme].name}</span>`;if(selectedLayout)previewHTML+=`<span style=\"background:#fef3c7;color:#d97706;padding:5px 10px;border-radius:15px;font-size:0.8rem;\">${layoutOptions[selectedLayout].name}</span>`;if(selectedColor)previewHTML+=`<span style=\"background:#f0fdf4;color:#059669;padding:5px 10px;border-radius:15px;font-size:0.8rem;\">${colorSchemes[selectedColor].name}</span>`;if(selectedFeatures)previewHTML+=`<span style=\"background:#fdf2f8;color:#be185d;padding:5px 10px;border-radius:15px;font-size:0.8rem;\">${featureOptions[selectedFeatures].name}</span>`;previewHTML+='</div>';preview.innerHTML=previewHTML;preview.style.display='block'}grid.innerHTML='';const sortedPackages=Object.keys(packages).sort((a,b)=>packageScores[b]-packageScores[a]);sortedPackages.forEach(key=>{const pkg=packages[key];const isRecommended=key===recommendedPackage;const card=document.createElement('div');card.className=`package-card ${isRecommended?'recommended':''}`;card.innerHTML=`<h4 class=\"package-name\">${pkg.name}</h4><div class=\"package-price\">${pkg.price}</div><p class=\"package-description\">${pkg.description}</p><ul class=\"package-features\">${pkg.features.map(feature=>`<li>${feature}</li>`).join('')}</ul><button class=\"select-btn\" onclick=\"selectPackage('${key}')\">${isRecommended?'Choose Recommended':'Select Package'}</button>`;grid.appendChild(card)});container.style.display='block';container.scrollIntoView({behavior:'smooth'})}\n\nfunction selectPackage(key){const pkg=packages[key];let message=`Perfect choice! You've selected the ${pkg.name}`;if(selectedTheme)message+=` with ${websiteThemes[selectedTheme].name} theme`;if(selectedLayout)message+=`, ${layoutOptions[selectedLayout].name}`;if(selectedColor)message+=`, ${colorSchemes[selectedColor].name}`;if(selectedFeatures)message+=`, and ${featureOptions[selectedFeatures].name}`;message+=`. Our team will contact you within 24 hours to discuss your project!`;addBotMessage(message);const params=new URLSearchParams({package:key,theme:selectedTheme||'',layout:selectedLayout||'',color:selectedColor||'',features:selectedFeatures||'',business_type:userAnswers.business_type||'',website_purpose:userAnswers.website_purpose||'',budget_range:userAnswers.budget_range||'',timeline:userAnswers.timeline||''});setTimeout(()=>{window.location.href='/pages/contact?'+params.toString()},2000)}\n\ndocument.addEventListener('click',function(event){['themeModal','layoutModal','colorModal','featuresModal'].forEach(id=>{const modal=document.getElementById(id);if(event.target===modal)closeModal(id)})});\n</script>",
