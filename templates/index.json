dvanced-preview', icon: '⚡', tags: ['Advanced', 'Enhanced', 'Powerful'], weight: {basic: 1, standard: 3, premium: 2}},\n  custom_features: {name: 'Archmage Arsenal', desc: 'Ultimate custom magic', preview: 'custom-preview', icon: '🔮', tags: ['Custom', 'Ultimate', 'Unique'], weight: {basic: 0, standard: 1, premium: 3}}\n};\n\nconst packages = {\n  basic: {name: 'Apprentice Package', price: '$1,499', duration: '2-3 weeks', description: 'Perfect for beginning your digital journey', features: ['Up to 5 magical pages', 'Mobile-responsive enchantments', 'Basic SEO spells', 'Contact form magic', '30 days of support', '2 rounds of revisions']},\n  standard: {name: 'Wizard Package', price: '$3,999', duration: '4-6 weeks', description: 'Ideal for growing your magical empire', features: ['Up to 15 powerful pages', 'Custom design & branding', 'Advanced SEO sorcery', 'E-commerce enchantments', 'Social media integration', '90 days of support', '4 rounds of revisions', 'Analytics crystal ball']},\n  premium: {name: 'Archmage Package', price: '$8,999', duration: '8-12 weeks', description: 'Ultimate power for digital dominion', features: ['Unlimited mystical pages', 'Custom magical functionality', 'Advanced integrations', 'Multi-language spells', 'Custom admin portal', '1 year of support', 'Unlimited revisions', 'Performance optimization', 'Security enchantments', 'Training included']}\n};\n\n// Game State\nlet currentLevel = 1;\nlet currentQuest = 0;\nlet userChoices = {};\nlet packageScores = {basic: 0, standard: 0, premium: 0};\nlet selectedTheme = null, selectedLayout = null, selectedColor = null, selectedFeatures = null;\n\n// Game Functions\nfunction startAdventure() {\n  document.getElementById('heroSection').style.display = 'none';\n  document.getElementById('adventureConsole').style.display = 'block';\n  updateLevel();\n  nextQuest();\n}\n\nfunction updateLevel() {\n  document.getElementById('currentLevel').textContent = currentLevel;\n  const xpPercent = (currentLevel / Object.keys(adventures).length) * 100;\n  document.getElementById('xpFill').style.width = xpPercent + '%';\n}\n\nfunction nextQuest() {\n  const questKeys = Object.keys(adventures);\n  if (currentQuest >= questKeys.length) {\n    showTreasure();\n    return;\n  }\n  \n  const questKey = questKeys[currentQuest];\n  const quest = adventures[questKey];\n  \n  updateCharacter(quest.character, quest.speech);\n  document.getElementById('questTitle').textContent = quest.title;\n  \n  if (quest.type === 'realm') {\n    showRealmButton(quest.realm, questKey);\n  } else {\n    showQuestOptions(quest.options, questKey);\n  }\n}\n\nfunction updateCharacter(emoji, speech) {\n  document.getElementById('character').textContent = emoji;\n  document.getElementById('characterSpeech').textContent = speech;\n}\n\nfunction showQuestOptions(options, questKey) {\n  const container = document.getElementById('questOptions');\n  container.innerHTML = '';\n  \n  options.forEach(option => {\n    const optionDiv = document.createElement('div');\n    optionDiv.className = 'quest-option';\n    optionDiv.innerHTML = `\n      <div style=\"font-size: 1.2rem; margin-bottom: 8px;\">${option.label}</div>\n      <div style=\"opacity: 0.8; font-size: 0.9rem;\">${option.desc}</div>\n    `;\n    optionDiv.onclick = () => selectOption(option, questKey);\n    container.appendChild(optionDiv);\n  });\n}\n\nfunction showRealmButton(realmId, questKey) {\n  const container = document.getElementById('questOptions');\n  container.innerHTML = `\n    <div class=\"quest-option\" onclick=\"openRealm('${realmId}', '${questKey}')\" style=\"background: linear-gradient(135deg, #4ecdc4, #44a08d); border-color: #4ecdc4;\">\n      <div style=\"font-size: 1.5rem; margin-bottom: 10px;\">🚪 Enter the Realm</div>\n      <div style=\"opacity: 0.9;\">Click to explore magical options!</div>\n    </div>\n  `;\n}\n\nfunction selectOption(option, questKey) {\n  userChoices[questKey] = option.value;\n  Object.keys(option.weight).forEach(pkg => {\n    packageScores[pkg] += option.weight[pkg];\n  });\n  \n  addToInventory(option.label);\n  currentLevel++;\n  currentQuest++;\n  updateLevel();\n  \n  setTimeout(() => {\n    nextQuest();\n  }, 1000);\n}\n\nfunction addToInventory(item) {\n  const container = document.getElementById('inventoryItems');\n  const itemDiv = document.createElement('div');\n  itemDiv.className = 'inventory-item';\n  itemDiv.textContent = item;\n  container.appendChild(itemDiv);\n}\n\nfunction openRealm(realmId, questKey) {\n  document.getElementById(realmId).style.display = 'flex';\n  \n  if (realmId === 'themeRealm') populateThemes();\n  else if (realmId === 'layoutRealm') populateLayouts();\n  else if (realmId === 'colorRealm') populateColors();\n  else if (realmId === 'featureRealm') populateFeatures();\n}\n\nfunction populateThemes() {\n  const grid = document.getElementById('themeGrid');\n  grid.innerHTML = '';\n  \n  Object.keys(themes).forEach(key => {\n    const theme = themes[key];\n    const card = document.createElement('div');\n    card.className = 'realm-card';\n    card.onclick = () => selectTheme(key);\n    card.innerHTML = `\n      <div class=\"card-preview\">\n        <div class=\"theme-preview ${theme.preview}\">\n          <div class=\"theme-browser\">\n            <div class=\"browser-dot\"></div>\n            <div class=\"browser-dot\"></div>\n            <div class=\"browser-dot\"></div>\n          </div>\n          <div class=\"theme-content\">\n            <div class=\"theme-block\"></div>\n            <div class=\"theme-block\"></div>\n            <div class=\"theme-block\"></div>\n          </div>\n        </div>\n      </div>\n      <div class=\"card-info\">\n        <h4>${theme.name}</h4>\n        <p>${theme.desc}</p>\n        <div class=\"card-tags\">\n          ${theme.tags.map(tag => `<span class=\"card-tag\">${tag}</span>`).join('')}\n        </div>\n      </div>\n    `;\n    grid.appendChild(card);\n  });\n}\n\nfunction populateLayouts() {\n  const grid = document.getElementById('layoutGrid');\n  grid.innerHTML = '';\n  \n  Object.keys(layouts).forEach(key => {\n    const layout = layouts[key];\n    const card = document.createElement('div');\n    card.className = 'realm-card';\n    card.onclick = () => selectLayout(key);\n    card.innerHTML = `\n      <div class=\"card-preview\">\n        <div class=\"layout-preview ${layout.preview}\">\n          <div class=\"header\"></div>\n          <div class=\"main\">\n            <div class=\"sidebar\"></div>\n            <div class=\"content\"></div>\n          </div>\n        </div>\n      </div>\n      <div class=\"card-info\">\n        <h4>${layout.name}</h4>\n        <p>${layout.desc}</p>\n        <div class=\"card-tags\">\n          ${layout.tags.map(tag => `<span class=\"card-tag\">${tag}</span>`).join('')}\n        </div>\n      </div>\n    `;\n    grid.appendChild(card);\n  });\n}\n\nfunction populateColors() {\n  const grid = document.getElementById('colorGrid');\n  grid.innerHTML = '';\n  \n  Object.keys(colors).forEach(key => {\n    const color = colors[key];\n    const card = document.createElement('div');\n    card.className = 'realm-card';\n    card.onclick = () => selectColor(key);\n    card.innerHTML = `\n      <div class=\"card-preview\">\n        <div class=\"color-preview ${color.preview}\">\n          <div class=\"color-strip\">DEEP</div>\n          <div class=\"color-strip\">RICH</div>\n          <div class=\"color-strip\">LIGHT</div>\n          <div class=\"color-strip\">SOFT</div>\n        </div>\n      </div>\n      <div class=\"card-info\">\n        <h4>${color.name}</h4>\n        <p>${color.desc}</p>\n        <div class=\"card-tags\">\n          ${color.tags.map(tag => `<span class=\"card-tag\">${tag}</span>`).join('')}\n        </div>\n      </div>\n    `;\n    grid.appendChild(card);\n  });\n}\n\nfunction populateFeatures() {\n  const grid = document.getElementById('featureGrid');\n  grid.innerHTML = '';\n  \n  Object.keys(features).forEach(key => {\n    const feature = features[key];\n    const card = document.createElement('div');\n    card.className = 'realm-card';\n    card.onclick = () => selectFeature(key);\n    card.innerHTML = `\n      <div class=\"card-preview\">\n        <div class=\"feature-preview ${feature.preview}\">\n          <div class=\"feature-icon\">${feature.icon}</div>\n          <div class=\"feature-name\">${feature.name}</div>\n        </div>\n      </div>\n      <div class=\"card-info\">\n        <h4>${feature.name}</h4>\n        <p>${feature.desc}</p>\n        <div class=\"card-tags\">\n          ${feature.tags.map(tag => `<span class=\"card-tag\">${tag}</span>`).join('')}\n        </div>\n      </div>\n    `;\n    grid.appendChild(card);\n  });\n}\n\nfunction selectTheme(key) {\n  selectedTheme = key;\n  const theme = themes[key];\n  Object.keys(theme.weight).forEach(pkg => {\n    packageScores[pkg] += theme.weight[pkg];\n  });\n  selectRealmItem('themeRealm', theme.name);\n}\n\nfunction selectLayout(key) {\n  selectedLayout = key;\n  const layout = layouts[key];\n  Object.keys(layout.weight).forEach(pkg => {\n    packageScores[pkg] += layout.weight[pkg];\n  });\n  selectRealmItem('layoutRealm', layout.name);\n}\n\nfunction selectColor(key) {\n  selectedColor = key;\n  const color = colors[key];\n  Object.keys(color.weight).forEach(pkg => {\n    packageScores[pkg] += color.weight[pkg];\n  });\n  selectRealmItem('colorRealm', color.name);\n}\n\nfunction selectFeature(key) {\n  selectedFeatures = key;\n  const feature = features[key];\n  Object.keys(feature.weight).forEach(pkg => {\n    packageScores[pkg] += feature.weight[pkg];\n  });\n  selectRealmItem('featureRealm', feature.name);\n}\n\nfunction selectRealmItem(realmId, itemName) {\n  document.querySelectorAll(`#${realmId} .realm-card`).forEach(card => {\n    card.classList.remove('selected');\n  });\n  event.currentTarget.classList.add('selected');\n  \n  setTimeout(() => {\n    closeRealm(realmId);\n    addToInventory(itemName);\n    currentLevel++;\n    currentQuest++;\n    updateLevel();\n    setTimeout(nextQuest, 800);\n  }, 1500);\n}\n\nfunction closeRealm(realmId) {\n  document.getElementById(realmId).style.display = 'none';\n}\n\nfunction showTreasure() {\n  document.getElementById('adventureConsole').style.display = 'none';\n  document.getElementById('treasureSection').style.display = 'block';\n  \n  // Show adventure summary\n  const summary = document.getElementById('adventureSummary');\n  let summaryHTML = '<h3>🎉 Your Epic Adventure Summary</h3><div class=\"summary-items\">';\n  \n  if (selectedTheme) summaryHTML += `<div class=\"summary-item\">🏰 ${themes[selectedTheme].name}</div>`;\n  if (selectedLayout) summaryHTML += `<div class=\"summary-item\">🌌 ${layouts[selectedLayout].name}</div>`;\n  if (selectedColor) summaryHTML += `<div class=\"summary-item\">💎 ${colors[selectedColor].name}</div>`;\n  if (selectedFeatures) summaryHTML += `<div class=\"summary-item\">🌳 ${features[selectedFeatures].name}</div>`;\n  \n  summaryHTML += '</div>';\n  summary.innerHTML = summaryHTML;\n  \n  // Show packages\n  displayPackages();\n}\n\nfunction displayPackages() {\n  const recommendedPackage = Object.keys(packageScores).reduce((a, b) => \n    packageScores[a] > packageScores[b] ? a : b\n  );\n  \n  const showcase = document.getElementById('packagesShowcase');\n  const sortedPackages = Object.keys(packages).sort((a, b) => packageScores[b] - packageScores[a]);\n  \n  showcase.innerHTML = '';\n  \n  sortedPackages.forEach(key => {\n    const pkg = packages[key];\n    const isRecommended = key === recommendedPackage;\n    \n    const card = document.createElement('div');\n    card.className = `package-card ${isRecommended ? 'recommended' : ''}`;\n    card.innerHTML = `\n      <h3 class=\"package-name\">${pkg.name}</h3>\n      <div class=\"package-price\">${pkg.price}</div>\n      <p class=\"package-description\">${pkg.description}</p>\n      <ul class=\"package-features\">\n        ${pkg.features.map(feature => `<li>${feature}</li>`).join('')}\n      </ul>\n      <button class=\"claim-btn\" onclick=\"claimPackage('${key}')\">\n        ${isRecommended ? '👑 Claim Your Destiny' : '⚡ Choose This Power'}\n      </button>\n    `;\n    showcase.appendChild(card);\n  });\n}\n\nfunction claimPackage(packageKey) {\n  const pkg = packages[packageKey];\n  \n  // Create magical effect\n  const btn = event.currentTarget;\n  btn.innerHTML = '✨ Claiming Your Power... ✨';\n  btn.style.background = 'linear-gradient(135deg, #ffd700, #ffed4e)';\n  \n  setTimeout(() => {\n    const params = new URLSearchParams({\n      package: packageKey,\n      theme: selectedTheme || '',\n      layout: selectedLayout || '',\n      color: selectedColor || '',\n      features: selectedFeatures || '',\n      business_type: userChoices.business_type || '',\n      website_purpose: userChoices.website_purpose || '',\n      budget_range: userChoices.budget_range || '',\n      timeline: userChoices.timeline || ''\n    });\n    \n    window.location.href = '/pages/contact?' + params.toString();\n  }, 2000);\n}\n\n// Initialize treasure chest animation\ndocument.addEventListener('DOMContentLoaded', function() {\n  // Add sparkle effects\n  setInterval(() => {\n    if (document.getElementById('treasureSection').style.display !== 'none') {\n      createSparkle();\n    }\n  }, 500);\n});\n\nfunction createSparkle() {\n  const sparkle = document.createElement('div');\n  sparkle.innerHTML = '✨';\n  sparkle.style.position = 'absolute';\n  sparkle.style.left = Math.random() * 100 + '%';\n  sparkle.style.top = Math.random() * 100 + '%';\n  sparkle.style.fontSize = '1.5rem';\n  sparkle.style.pointerEvents = 'none';\n  sparkle.style.animation = 'sparkleFloat 3s ease-out forwards';\n  \n  document.querySelector('.treasure-section').appendChild(sparkle);\n  \n  setTimeout(() => {\n    sparkle.remove();\n  }, 3000);\n}\n\n// Add sparkle animation\nconst style = document.createElement('style');\nstyle.textContent = `\n  @keyframes sparkleFloat {\n    0% { opacity: 1; transform: translateY(0px) scale(0); }\n    50% { opacity: 1; transform: translateY(-50px) scale(1); }\n    100% { opacity: 0; transform: translateY(-100px) scale(0); }\n  }\n`;\ndocument.head.appendChild(style);\n</script>",
