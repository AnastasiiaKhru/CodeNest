    "custom_liquid_ahmdFX": {
      "type": "custom-liquid",
      "name": "t:sections.custom-liquid.presets.name",
      "settings": {
        "custom_liquid": "<div class=\"liquid-form-container\">\n\n  <style>\n    /* All your existing styles remain the same */\n    .liquid-form-container {\n      max-width: 800px;\n      margin: 2rem auto;\n      padding: 2rem;\n      background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\n      border-radius: 20px;\n      box-shadow: 0 20px 40px rgba(0,0,0,0.1);\n      position: relative;\n      overflow: hidden;\n    }\n    \n    /* ... rest of your existing styles ... */\n    \n    .success-message {\n      background: #d4edda;\n      color: #155724;\n      padding: 1rem;\n      border-radius: 10px;\n      margin-bottom: 1rem;\n      border: 1px solid #c3e6cb;\n    }\n    \n    .error-message {\n      background: #f8d7da;\n      color: #721c24;\n      padding: 1rem;\n      border-radius: 10px;\n      margin-bottom: 1rem;\n      border: 1px solid #f5c6cb;\n    }\n  </style>\n\n  <div class=\"form-header\">\n    <h1 class=\"form-title\">Let's Build Your Dream Website! ✨</h1>\n    <p class=\"form-subtitle\">Tell us about your vision and we'll create something amazing together</p>\n  </div>\n\n  &lt;!-- Show success/error messages -->\n  {% if form.posted_successfully? %}\n    <div class=\"success-message\">\n      ✅ Thank you! Your website preferences have been submitted successfully. We'll contact you within 24 hours!\n    </div>\n  {% endif %}\n\n  {% if form.errors %}\n    <div class=\"error-message\">\n      ❌ Please fix the following errors:\n      <ul>\n        {% for field in form.errors %}\n          <li>{{ field | first | capitalize }} {{ field | last }}</li>\n        {% endfor %}\n      </ul>\n    </div>\n  {% endif %}\n\n \n  {% form 'contact', class: 'liquid-form' %}\n    \n    \n    <input type=\"hidden\" name=\"contact[subject]\" value=\"🎨 Website Design Request - {{ customer.email | default: 'New Customer' }}\">\n    <input type=\"hidden\" name=\"contact[form_type]\" value=\"website_design_form\">\n    <input type=\"hidden\" name=\"contact[timestamp]\" value=\"{{ 'now' | date: '%B %d, %Y at %I:%M %p' }}\">\n\n    &lt;!-- Personal Information -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">👤</span>\n        Personal Information\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_name\">Full Name *</label>\n        <input type=\"text\" \n               id=\"contact_name\" \n               name=\"contact[name]\" \n               class=\"form-input\" \n               required \n               value=\"{{ form.name | default: customer.name }}\">\n        {% if form.errors contains 'name' %}\n          <span class=\"error\">{{ form.errors.name }}</span>\n        {% endif %}\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_email\">Email Address *</label>\n        <input type=\"email\" \n               id=\"contact_email\" \n               name=\"contact[email]\" \n               class=\"form-input\" \n               required \n               value=\"{{ form.email | default: customer.email }}\">\n        {% if form.errors contains 'email' %}\n          <span class=\"error\">{{ form.errors.email }}</span>\n        {% endif %}\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_phone\">Phone Number</label>\n        <input type=\"tel\" \n               id=\"contact_phone\" \n               name=\"contact[phone]\" \n               class=\"form-input\" \n               value=\"{{ form.phone | default: customer.phone }}\">\n      </div>\n    </div>\n\n    &lt;!-- Website Theme Selection -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🎨</span>\n        Choose Your Website Theme\n      </h3>\n      <div class=\"theme-grid\">\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"theme_modern\" name=\"contact[theme]\" value=\"Modern Minimalist\" required>\n          <label for=\"theme_modern\" class=\"theme-card\">\n            <strong>Modern Minimalist</strong>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #000;\"></div>\n              <div class=\"color-dot\" style=\"background: #fff;\"></div>\n              <div class=\"color-dot\" style=\"background: #666;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"theme_vibrant\" name=\"contact[theme]\" value=\"Vibrant Creative\">\n          <label for=\"theme_vibrant\" class=\"theme-card\">\n            <strong>Vibrant Creative</strong>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #ff6b6b;\"></div>\n              <div class=\"color-dot\" style=\"background: #4ecdc4;\"></div>\n              <div class=\"color-dot\" style=\"background: #45b7d1;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"theme_elegant\" name=\"contact[theme]\" value=\"Elegant Professional\">\n          <label for=\"theme_elegant\" class=\"theme-card\">\n            <strong>Elegant Professional</strong>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #2c3e50;\"></div>\n              <div class=\"color-dot\" style=\"background: #3498db;\"></div>\n              <div class=\"color-dot\" style=\"background: #ecf0f1;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"theme_warm\" name=\"contact[theme]\" value=\"Warm & Cozy\">\n          <label for=\"theme_warm\" class=\"theme-card\">\n            <strong>Warm & Cozy</strong>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #d4a574;\"></div>\n              <div class=\"color-dot\" style=\"background: #8b4513;\"></div>\n              <div class=\"color-dot\" style=\"background: #f4e4c1;\"></div>\n            </div>\n          </label>\n        </div>\n      </div>\n    </div>\n\n    &lt;!-- Website Details -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🌐</span>\n        Website Details\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"website_purpose\">What's the main purpose of your website? *</label>\n        <select id=\"website_purpose\" name=\"contact[website_purpose]\" class=\"form-select\" required>\n          <option value=\"\">Select purpose...</option>\n          <option value=\"Business/Corporate\">Business/Corporate</option>\n          <option value=\"E-commerce Store\">E-commerce Store</option>\n          <option value=\"Portfolio/Personal\">Portfolio/Personal</option>\n          <option value=\"Blog/Content\">Blog/Content</option>\n          <option value=\"Non-profit/Charity\">Non-profit/Charity</option>\n          <option value=\"Restaurant/Food\">Restaurant/Food</option>\n          <option value=\"Creative/Art\">Creative/Art</option>\n          <option value=\"Other\">Other</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"business_name\">Business/Website Name *</label>\n        <input type=\"text\" id=\"business_name\" name=\"contact[business_name]\" class=\"form-input\" required>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"website_description\">Describe your business/website in a few sentences *</label>\n        <textarea id=\"website_description\" \n                  name=\"contact[body]\" \n                  class=\"form-textarea\" \n                  rows=\"4\" \n                  required \n                  placeholder=\"Tell us what you do, who your target audience is, and what makes you unique...\">{{ form.body }}</textarea>\n        {% if form.errors contains 'body' %}\n          <span class=\"error\">{{ form.errors.body }}</span>\n        {% endif %}\n      </div>\n    </div>\n\n    &lt;!-- Features & Functionality -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">⚡</span>\n        Features & Functionality\n      </h3>\n      <p class=\"form-label\">What features do you need on your website? (Select all that apply)</p>\n      \n      &lt;!-- Collect features as a single text field for better email formatting -->\n      <input type=\"hidden\" id=\"selected_features\" name=\"contact[features_needed]\" value=\"\">\n      \n      <div class=\"checkbox-group\">\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Contact Form\">\n          <span>Contact Form</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Online Store\">\n          <span>Online Store</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Blog Section\">\n          <span>Blog Section</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Photo Gallery\">\n          <span>Photo Gallery</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Social Media Integration\">\n          <span>Social Media Integration</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Newsletter Signup\">\n          <span>Newsletter Signup</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Online Booking\">\n          <span>Online Booking</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Customer Reviews\">\n          <span>Customer Reviews</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Multi-language Support\">\n          <span>Multi-language Support</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Live Chat\">\n          <span>Live Chat</span>\n        </label>\n      </div>\n    </div>\n\n    &lt;!-- Design Preferences -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🎯</span>\n        Design Preferences\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"color_preferences\">Preferred Colors</label>\n        <input type=\"text\" \n               id=\"color_preferences\" \n               name=\"contact[color_preferences]\" \n               class=\"form-input\" \n               placeholder=\"e.g., Blue and white, Earth tones, Bright and colorful...\">\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"style_inspiration\">Website Inspiration</label>\n        <textarea id=\"style_inspiration\" \n                  name=\"contact[style_inspiration]\" \n                  class=\"form-textarea\" \n                  rows=\"3\" \n                  placeholder=\"Share URLs of websites you admire or describe the style you're looking for...\"></textarea>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"special_requirements\">Additional Notes</label>\n        <textarea id=\"special_requirements\" \n                  name=\"contact[special_requirements]\" \n                  class=\"form-textarea\" \n                  rows=\"4\" \n                  placeholder=\"Any specific requirements, accessibility needs, or additional information...\"></textarea>\n      </div>\n    </div>\n\n    &lt;!-- Order Information (if available) -->\n    {% if order %}\n      <input type=\"hidden\" name=\"contact[order_number]\" value=\"{{ order.name }}\">\n      <input type=\"hidden\" name=\"contact[order_total]\" value=\"{{ order.total_price | money }}\">\n    {% endif %}\n\n    <div class=\"submit-section\">\n      <button type=\"submit\" class=\"submit-btn\">\n        🚀 Submit My Website Preferences\n      </button>\n      <p style=\"margin-top: 1rem; color: #666; font-size: 0.9rem;\">\n        We'll review your preferences and get back to you within 24 hours!\n      </p>\n    </div>\n\n  {% endform %}\n\n  <div class=\"liquid-wave\"></div>\n</div>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  // Collect selected features into hidden field\n  function updateSelectedFeatures() {\n    const checkboxes = document.querySelectorAll('.feature-checkbox:checked');\n    const selectedFeatures = Array.from(checkboxes).map(cb => cb.value).join(', ');\n    document.getElementById('selected_features').value = selectedFeatures || 'None selected';\n  }\n\n  // Listen for checkbox changes\n  document.querySelectorAll('.feature-checkbox').forEach(checkbox => {\n    checkbox.addEventListener('change', updateSelectedFeatures);\n  });\n\n  // Your existing animation code...\n  const sections = document.querySelectorAll('.form-section');\n  const observer = new IntersectionObserver((entries) => {\n    entries.forEach(entry => {\n      if (entry.isIntersecting) {\n        entry.target.style.animationPlayState = 'running';\n      }\n    });\n  });\n\n  sections.forEach(section => {\n    observer.observe(section);\n  });\n\n  // Submit button effects\n  const submitBtn = document.querySelector('.submit-btn');\n  if (submitBtn) {\n    submitBtn.addEventListener('click', function(e) {\n      updateSelectedFeatures(); // Make sure features are collected before submit\n      \n      const ripple = document.createElement('span');\n      const rect = this.getBoundingClientRect();\n      const size = Math.max(rect.width, rect.height);\n      const x = e.clientX - rect.left - size / 2;\n      const y = e.clientY - rect.top - size / 2;\n      \n      ripple.style.cssText = `\n        position: absolute;\n        width: ${size}px;\n        height: ${size}px;\n        left: ${x}px;\n        top: ${y}px;\n        background: rgba(255,255,255,0.3);\n        border-radius: 50%;\n        transform: scale(0);\n        animation: ripple 0.6s ease-out;\n        pointer-events: none;\n      `;\n      \n      this.appendChild(ripple);\n      setTimeout(() => ripple.remove(), 600);\n    });\n  }\n});\n\n// Add ripple animation CSS\nconst style = document.createElement('style');\nstyle.textContent = `\n  @keyframes ripple {\n    to {\n      transform: scale(2);\n      opacity: 0;\n    }\n  }\n  .submit-btn {\n    position: relative;\n    overflow: hidden;\n  }\n  .error {\n    color: #dc3545;\n    font-size: 0.875rem;\n    margin-top: 0.25rem;\n    display: block;\n  }\n`;\ndocument.head.appendChild(style);\n</script>",
