isplay: flex;\n      justify-content: center;\n      gap: 0.5rem;\n      margin-top: 1rem;\n    }\n    \n    .color-dot {\n      width: 20px;\n      height: 20px;\n      border-radius: 50%;\n      border: 2px solid rgba(255, 255, 255, 0.8);\n      box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\n    }\n    \n    .checkbox-group {\n      display: grid;\n      grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\n      gap: 1rem;\n      margin-top: 1rem;\n    }\n    \n    .checkbox-item {\n      display: flex;\n      align-items: center;\n      padding: 1rem;\n      background: linear-gradient(135deg, #fff 0%, #f0f9ff 100%);\n      border: 2px solid #a9dfbf;\n      border-radius: 15px;\n      cursor: pointer;\n      transition: all 0.3s ease;\n      position: relative;\n    }\n    \n    .checkbox-item:hover {\n      background: linear-gradient(135deg, #f0f9ff 0%, #eafaf1 100%);\n      transform: translateY(-2px);\n      box-shadow: 0 5px 15px rgba(169, 223, 191, 0.3);\n    }\n    \n    .checkbox-item input[type=\"checkbox\"] {\n      margin-right: 0.75rem;\n      transform: scale(1.2);\n      accent-color: #27ae60;\n    }\n    \n    .checkbox-item input[type=\"checkbox\"]:checked + span::after {\n      content: ' 🧘‍♀️';\n      animation: sparkleIn 0.5s ease-out;\n    }\n    \n    @keyframes sparkleIn {\n      0% { opacity: 0; transform: scale(0); }\n      100% { opacity: 1; transform: scale(1); }\n    }\n    \n    .submit-section {\n      text-align: center;\n      margin-top: 3rem;\n    }\n    \n    .submit-btn {\n      background: linear-gradient(135deg, #58d68d 0%, #27ae60 50%, #1e8449 100%);\n      color: white;\n      border: none;\n      padding: 1.2rem 3rem;\n      font-size: 1.2rem;\n      font-weight: 600;\n      border-radius: 50px;\n      cursor: pointer;\n      transition: all 0.3s ease;\n      font-family: 'Quicksand', sans-serif;\n      box-shadow: 0 10px 30px rgba(39, 174, 96, 0.3);\n      position: relative;\n      overflow: hidden;\n    }\n    \n    .submit-btn:disabled {\n      background: linear-gradient(135deg, #bdc3c7 0%, #95a5a6 50%, #7f8c8d 100%);\n      cursor: not-allowed;\n      opacity: 0.6;\n    }\n    \n    .submit-btn::before {\n      content: '';\n      position: absolute;\n      top: 0;\n      left: -100%;\n      width: 100%;\n      height: 100%;\n      background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);\n      transition: left 0.5s;\n    }\n    \n    .submit-btn:hover:not(:disabled) {\n      transform: translateY(-3px);\n      box-shadow: 0 15px 40px rgba(39, 174, 96, 0.4);\n    }\n    \n    .submit-btn:hover:not(:disabled)::before {\n      left: 100%;\n    }\n    \n    .success-message {\n      background: linear-gradient(135deg, #c8e6c9 0%, #a5d6a7 100%);\n      color: #2e7d32;\n      padding: 1.5rem;\n      border-radius: 20px;\n      margin-bottom: 2rem;\n      border: 2px solid #81c784;\n      text-align: center;\n      font-weight: 500;\n    }\n    \n    .error-message {\n      background: linear-gradient(135deg, #ffcdd2 0%, #ef9a9a 100%);\n      color: #c62828;\n      padding: 1.5rem;\n      border-radius: 20px;\n      margin-bottom: 2rem;\n      border: 2px solid #e57373;\n    }\n    \n    .error {\n      color: #27ae60;\n      font-size: 0.875rem;\n      margin-top: 0.5rem;\n      display: block;\n      font-weight: 500;\n    }\n    \n    .captcha-message {\n      font-size: 0.875rem;\n      margin-top: 0.5rem;\n      display: block;\n      font-weight: 500;\n    }\n    \n    .captcha-message.success {\n      color: #2e7d32;\n    }\n    \n    .captcha-message.error {\n      color: #c62828;\n    }\n    \n    .captcha-box {\n      background: linear-gradient(135deg, #fff9c4 0%, #f7dc6f 100%);\n      border: 2px solid #f39c12;\n      border-radius: 15px;\n      padding: 1.5rem;\n      margin: 1rem 0;\n      text-align: center;\n    }\n    \n    .captcha-question {\n      font-size: 1.1rem;\n      font-weight: 600;\n      color: #d68910;\n      margin-bottom: 1rem;\n    }\n    \n    .captcha-input {\n      width: 100px;\n      padding: 0.5rem;\n      border: 2px solid #f39c12;\n      border-radius: 10px;\n      text-align: center;\n      font-size: 1.1rem;\n      font-weight: 600;\n    }\n    \n    .floating-hearts {\n      position: absolute;\n      top: 0;\n      left: 0;\n      width: 100%;\n      height: 100%;\n      pointer-events: none;\n      overflow: hidden;\n    }\n    \n    .floating-hearts::before,\n    .floating-hearts::after {\n      content: '🧘‍♀️';\n      position: absolute;\n      font-size: 2rem;\n      animation: float 6s ease-in-out infinite;\n      opacity: 0.3;\n    }\n    \n    .floating-hearts::before {\n      top: 20%;\n      left: 10%;\n      animation-delay: 0s;\n    }\n    \n    .floating-hearts::after {\n      top: 60%;\n      right: 15%;\n      animation-delay: 3s;\n    }\n    \n    @keyframes float {\n      0%, 100% { transform: translateY(0px) rotate(0deg); }\n      50% { transform: translateY(-20px) rotate(180deg); }\n    }\n    \n    @media (max-width: 768px) {\n      .cute-form-container {\n        margin: 1rem;\n        padding: 2rem;\n      }\n      \n      .form-title {\n        font-size: 2.2rem;\n      }\n      \n      .theme-grid {\n        grid-template-columns: 1fr;\n      }\n      \n      .checkbox-group {\n        grid-template-columns: 1fr;\n      }\n    }\n  </style>\n\n  <div class=\"floating-hearts\"></div>\n\n  <div class=\"form-header\">\n    <h1 class=\"form-title\">Begin Your Yoga Journey! 🧘‍♀️</h1>\n    <div class=\"cute-hearts\">🕉️ ✨ 🌸 ✨ 🕉️</div>\n    <p class=\"form-subtitle\">Share your wellness goals and let us guide you to inner peace and strength! 🌿</p>\n  </div>\n\n  <!-- Show success/error messages -->\n  {% if form.posted_successfully? %}\n    <div class=\"success-message\">\n      🙏 Namaste! Your yoga inquiry has been submitted successfully! We'll connect with you within 24 hours to discuss your journey! 🧘‍♀️✨\n    </div>\n  {% endif %}\n\n  {% if form.errors %}\n    <div class=\"error-message\">\n      🌱 Gentle reminder! Please address these mindful adjustments:\n      <ul>\n        {% for field in form.errors %}\n          <li>{{ field | first | capitalize }} {{ field | last }}</li>\n        {% endfor %}\n      </ul>\n    </div>\n  {% endif %}\n\n  <!-- CAPTCHA Section - BEFORE the form -->\n  <div class=\"form-section\">\n    <h3 class=\"section-title\">\n      <span class=\"section-icon\">🔒</span>\n      Security Check - Prove You're Human!\n    </h3>\n    <div class=\"captcha-box\">\n      <div class=\"captcha-question\" id=\"captcha_question\">What is ...?</div>\n      <input type=\"number\" \n             id=\"captcha_answer\" \n             class=\"captcha-input\" \n             placeholder=\"?\">\n      <div id=\"captcha_message\" class=\"captcha-message\" style=\"display: none;\"></div>\n    </div>\n  </div>\n\n  {% form 'contact', class: 'liquid-form' %}\n    \n    <input type=\"hidden\" name=\"contact[subject]\" value=\"🧘‍♀️ Yoga Journey Inquiry - {{ customer.email | default: 'New Student' }}\">\n    <input type=\"hidden\" name=\"contact[form_type]\" value=\"yoga_inquiry_form\">\n    <input type=\"hidden\" name=\"contact[timestamp]\" value=\"{{ 'now' | date: '%B %d, %Y at %I:%M %p' }}\">\n\n    <!-- Personal Information -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🌸</span>\n        Tell Us About Yourself, Beautiful Soul!\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_name\">Your Sacred Name *</label>\n        <input type=\"text\" \n               id=\"contact_name\" \n               name=\"contact[name]\" \n               class=\"form-input\" \n               required \n               placeholder=\"What should we call you, dear yogi?\"\n               value=\"{{ form.name | default: customer.name }}\">\n        {% if form.errors contains 'name' %}\n          <span class=\"error\">{{ form.errors.name }}</span>\n        {% endif %}\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_email\">Your Email Address *</label>\n        <input type=\"email\" \n               id=\"contact_email\" \n               name=\"contact[email]\" \n               class=\"form-input\" \n               required \n               placeholder=\"your.email@example.com\"\n               value=\"{{ form.email | default: customer.email }}\">\n        {% if form.errors contains 'email' %}\n          <span class=\"error\">{{ form.errors.email }}</span>\n        {% endif %}\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"contact_phone\">Phone Number (Optional)</label>\n        <input type=\"tel\" \n               id=\"contact_phone\" \n               name=\"contact[phone]\" \n               class=\"form-input\" \n               placeholder=\"So we can connect about your yoga journey!\"\n               value=\"{{ form.phone | default: customer.phone }}\">\n      </div>\n    </div>\n\n    <!-- Yoga Style Preference -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🎨</span>\n        Choose Your Yoga Path\n      </h3>\n      <div class=\"theme-grid\">\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"style_hatha\" name=\"contact[yoga_style]\" value=\"Hatha Yoga - Gentle & Grounding\" required>\n          <label for=\"style_hatha\" class=\"theme-card\">\n            <strong>Hatha Yoga</strong>\n            <p style=\"font-size: 0.9rem; margin-top: 0.5rem;\">Gentle & Grounding</p>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #8fbc8f;\"></div>\n              <div class=\"color-dot\" style=\"background: #deb887;\"></div>\n              <div class=\"color-dot\" style=\"background: #f5f5dc;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"style_vinyasa\" name=\"contact[yoga_style]\" value=\"Vinyasa Flow - Dynamic & Flowing\">\n          <label for=\"style_vinyasa\" class=\"theme-card\">\n            <strong>Vinyasa Flow</strong>\n            <p style=\"font-size: 0.9rem; margin-top: 0.5rem;\">Dynamic & Flowing</p>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #4682b4;\"></div>\n              <div class=\"color-dot\" style=\"background: #87ceeb;\"></div>\n              <div class=\"color-dot\" style=\"background: #e0f6ff;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"style_yin\" name=\"contact[yoga_style]\" value=\"Yin Yoga - Deep & Restorative\">\n          <label for=\"style_yin\" class=\"theme-card\">\n            <strong>Yin Yoga</strong>\n            <p style=\"font-size: 0.9rem; margin-top: 0.5rem;\">Deep & Restorative</p>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #dda0dd;\"></div>\n              <div class=\"color-dot\" style=\"background: #e6e6fa;\"></div>\n              <div class=\"color-dot\" style=\"background: #f8f8ff;\"></div>\n            </div>\n          </label>\n        </div>\n        <div class=\"theme-option\">\n          <input type=\"radio\" id=\"style_power\" name=\"contact[yoga_style]\" value=\"Power Yoga - Strong & Energizing\">\n          <label for=\"style_power\" class=\"theme-card\">\n            <strong>Power Yoga</strong>\n            <p style=\"font-size: 0.9rem; margin-top: 0.5rem;\">Strong & Energizing</p>\n            <div class=\"color-palette\">\n              <div class=\"color-dot\" style=\"background: #ff6347;\"></div>\n              <div class=\"color-dot\" style=\"background: #ffa500;\"></div>\n              <div class=\"color-dot\" style=\"background: #ffd700;\"></div>\n            </div>\n          </label>\n        </div>\n      </div>\n    </div>\n\n    <!-- Experience & Goals -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🌟</span>\n        Your Yoga Experience & Goals\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"experience_level\">What's your current yoga experience? *</label>\n        <select id=\"experience_level\" name=\"contact[experience_level]\" class=\"form-select\" required>\n          <option value=\"\">Choose your level...</option>\n          <option value=\"Complete Beginner\">Complete Beginner - New to yoga 🌱</option>\n          <option value=\"Some Experience\">Some Experience - Practiced a few times 🌿</option>\n          <option value=\"Regular Practitioner\">Regular Practitioner - Practice weekly 🌸</option>\n          <option value=\"Experienced Yogi\">Experienced Yogi - Daily practice 🧘‍♀️</option>\n          <option value=\"Advanced/Teacher\">Advanced/Teacher - Teaching or advanced practice 🕉️</option>\n        </select>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"yoga_goals\">What are your yoga goals? *</label>\n        <input type=\"text\" \n               id=\"yoga_goals\" \n               name=\"contact[yoga_goals]\" \n               class=\"form-input\" \n               placeholder=\"Flexibility, stress relief, strength, spiritual growth...\"\n               required>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"yoga_story\">Tell us about your wellness journey! *</label>\n        <textarea id=\"yoga_story\" \n                  name=\"contact[body]\" \n                  class=\"form-textarea\" \n                  rows=\"4\" \n                  required \n                  placeholder=\"Share what brought you to yoga, any challenges you're facing, or what you hope to achieve...\">{{ form.body }}</textarea>\n        {% if form.errors contains 'body' %}\n          <span class=\"error\">{{ form.errors.body }}</span>\n        {% endif %}\n      </div>\n    </div>\n\n    <!-- Class Preferences -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">✨</span>\n        Your Class Preferences\n      </h3>\n      <p class=\"form-label\">What type of classes interest you? (Select all that resonate!)</p>\n      \n      <input type=\"hidden\" id=\"selected_features\" name=\"contact[class_preferences]\" value=\"\">\n      \n      <div class=\"checkbox-group\">\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Beginner Classes\">\n          <span>Beginner Classes 🌱</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Morning Flow\">\n          <span>Morning Flow 🌅</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Evening Relaxation\">\n          <span>Evening Relaxation 🌙</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Meditation Sessions\">\n          <span>Meditation Sessions 🧘‍♀️</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Prenatal Yoga\">\n          <span>Prenatal Yoga 🤱</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Senior Yoga\">\n          <span>Senior Yoga 👵</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Hot Yoga\">\n          <span>Hot Yoga 🔥</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Yoga Therapy\">\n          <span>Yoga Therapy 💚</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Private Sessions\">\n          <span>Private Sessions 👤</span>\n        </label>\n        <label class=\"checkbox-item\">\n          <input type=\"checkbox\" class=\"feature-checkbox\" value=\"Workshops & Retreats\">\n          <span>Workshops & Retreats 🏔️</span>\n        </label>\n      </div>\n    </div>\n\n    <!-- Health & Wellness -->\n    <div class=\"form-section\">\n      <h3 class=\"section-title\">\n        <span class=\"section-icon\">🎀</span>\n        Health & Wellness Information\n      </h3>\n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"health_conditions\">Any health conditions or injuries we should know about?</label>\n        <input type=\"text\" \n               id=\"health_conditions\" \n               name=\"contact[health_conditions]\" \n               class=\"form-input\" \n               placeholder=\"Back pain, knee issues, pregnancy, etc. (helps us modify poses)\">\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"availability\">When are you available for classes?</label>\n        <textarea id=\"availability\" \n                  name=\"contact[availability]\" \n                  class=\"form-textarea\" \n                  rows=\"3\" \n                  placeholder=\"Mornings, evenings, weekends, specific days...\"></textarea>\n      </div>\n      \n      <div class=\"form-group\">\n        <label class=\"form-label\" for=\"special_requests\">Any special requests or questions?</label>\n        <textarea id=\"special_requests\" \n                  name=\"contact[special_requests]\" \n                  class=\"form-textarea\" \n                  rows=\"4\" \n                  placeholder=\"Questions about our studio, specific needs, or anything else you'd like us to know...\"></textarea>\n      </div>\n    </div>\n\n    <!-- Order Information (if available) -->\n    {% if order %}\n      <input type=\"hidden\" name=\"contact[order_number]\" value=\"{{ order.name }}\">\n      <input type=\"hidden\" name=\"contact[order_total]\" value=\"{{ order.total_price | money }}\">\n    {% endif %}\n\n    <div class=\"submit-section\">\n      <button type=\"submit\" class=\"submit-btn\" id=\"submit_button\" disabled>\n        🧘‍♀️ Begin My Yoga Journey 🧘‍♀️\n      </button>\n      <p style=\"margin-top: 1.5rem; color: #1e8449; font-size: 1rem; font-weight: 500;\">\n        We'll mindfully review your information and connect with you within 24 hours to discuss your perfect yoga path! 🌿✨\n      </p>\n    </div>\n\n  {% endform %}\n\n</div>\n\n<script>\ndocument.addEventListener('DOMContentLoaded', function() {\n  // Simple CAPTCHA - Just basic math to ensure emails work\n  const captchaQuestion = document.getElementById('captcha_question');\n  const captchaAnswerInput = document.getElementById('captcha_answer');\n  const captchaMessage = document.getElementById('captcha_message');\n  const submitBtn = document.getElementById('submit_button');\n\n  let correctAnswer;\n\n  function generateCaptcha() {\n    const num1 = Math.floor(Math.random() * 20) + 5; // 5-24\n    const num2 = Math.floor(Math.random() * 20) + 5; // 5-24\n    const operations = ['+', '-'];\n    const operation = operations[Math.floor(Math.random() * operations.length)];\n    \n    if (operation === '+') {\n      correctAnswer = num1 + num2;\n      captchaQuestion.textContent = `What is ${num1} + ${num2}?`;\n    } else {\n      // Ensure positive result for subtraction\n      const larger = Math.max(num1, num2);\n      const smaller = Math.min(num1, num2);\n      correctAnswer = larger - smaller;\n      captchaQuestion.textContent = `What is ${larger} - ${smaller}?`;\n    }\n    \n    captchaAnswerInput.value = '';\n    captchaMessage.style.display = 'none';\n    submitBtn.disabled = true;\n  }\n\n  function checkCaptcha() {\n    const userAnswer = parseInt(captchaAnswerInput.value, 10);\n\n    if (isNaN(userAnswer)) {\n      captchaMessage.textContent = 'Please enter a number.';\n      captchaMessage.className = 'captcha-message error';\n      captchaMessage.style.display = 'block';\n      submitBtn.disabled = true;\n      return false;\n    }\n\n    if (userAnswer === correctAnswer) {\n      captchaMessage.textContent = '✅ Correct! You can now submit the form.';\n      captchaMessage.className = 'captcha-message success';\n      captchaMessage.style.display = 'block';\n      submitBtn.disabled = false;\n      return true;\n    } else {\n      captchaMessage.textContent = '❌ Incorrect. Please try the new problem.';\n      captchaMessage.className = 'captcha-message error';\n      captchaMessage.style.display = 'block';\n      generateCaptcha();\n      submitBtn.disabled = true;\n      return false;\n    }\n  }\n\n  // Initialize CAPTCHA\n  generateCaptcha();\n\n  // Event listeners for CAPTCHA\n  captchaAnswerInput.addEventListener('input', checkCaptcha);\n\n  // Collect selected features into hidden field\n  function updateSelectedFeatures() {\n    const checkboxes = document.querySelectorAll('.feature-checkbox:checked');\n    const selectedFeatures = Array.from(checkboxes).map(cb => cb.value).join(', ');\n    document.getElementById('selected_features').value = selectedFeatures || 'None selected';\n  }\n\n  // Listen for checkbox changes\n  document.querySelectorAll('.feature-checkbox').forEach(checkbox => {\n    checkbox.addEventListener('change', updateSelectedFeatures);\n  });\n\n  // Submit button effects\n  if (submitBtn) {\n    submitBtn.addEventListener('click', function(e) {\n      updateSelectedFeatures();\n      \n      // Only prevent submission if CAPTCHA not solved\n      if (submitBtn.disabled) {\n        e.preventDefault();\n        captchaMessage.textContent = '🔒 Please solve the math problem first.';\n        captchaMessage.className = 'captcha-message error';\n        captchaMessage.style.display = 'block';\n        captchaAnswerInput.focus();\n        return false;\n      }\n      \n      // If CAPTCHA is solved, let the form submit normally\n      // No preventDefault() here - let Shopify handle the submission\n    });\n  }\n\n  // Peaceful animations for form sections\n  const sections = document.querySelectorAll('.form-section');\n  const observer = new IntersectionObserver((entries) => {\n    entries.forEach(entry => {\n      if (entry.isIntersecting) {\n        entry.target.style.transform = 'translateY(0)';\n        entry.target.style.opacity = '1';\n      }\n    });\n  });\n\n  sections.forEach(section => {\n    section.style.transform = 'translateY(20px)';\n    section.style.opacity = '0';\n    section.style.transition = 'all 0.6s ease';\n    observer.observe(section);\n  });\n});\n</script>",
